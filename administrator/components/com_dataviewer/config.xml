<?xml version="1.0" encoding="utf-8"?>
<config>
	<params>
		<param name="record_display_limit" type="list" default="10" label="Record Display Limit" description="Number of records to display">
			<option value="5">5</option>
			<option value="10">10</option>
			<option value="25">25</option>
			<option value="50">50</option>
			<option value="100">100</option>
		</param>

		<param name="processing_mode_switch" type="list" default="0" label="Dynamic Processing mode" description="Dynamically switch between server-side and client-side processing modes">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>

		<param name="proc_switch_threshold" type="text"	label="Client-side threshold" description="Maximum number of cells to allow before switching to server-side processing" default="25000" />

		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="acl_users" type="text"	label="ACL Users" description="" default="" />
		<param name="acl_groups" type="text"	label="ACL Groups" description="" default="" />
	</params>
<config>

